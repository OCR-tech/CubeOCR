/*===================================================*/
QWidget #window {
  background-color: green;
  border-radius: $Border5;
  border: 1px solid red;
}


/*===================================================*/
#WidgetWindow {
  border-top-left-radius: $Border5;
  border-top-right-radius: $Border5;
}


/*===================================================*/
#labelw1 {
  background-color: $Color_TB;
}



/*===================================================*/
#labelw2 {
