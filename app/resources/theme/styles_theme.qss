}




/*===================================================*/
/*  General   */
/*===================================================*/

/*===================================================*/
QGroupBox {
  color: $Color_font;
  background-color: $Color_FG;
  border: 1px solid $Color_BD;
  border-radius: $Border3;
}

QGroupBox::title {
  background-color: $Color_FG;
}


/*===================================================*/
QScrollBar:horizontal {
  background: $Color_TB;
}

QScrollBar:vertical {
  background: $Color_TB;
}



/*===================================================*/
QTabWidget::pane {
    color: $Color_font;
    border: 1px solid $Color_BD;
    border-radius: $Border3;
    background-color: $Color_BG;
}

QTabBar::tab {
  color: $Color_font;
  border-top: 1px solid $Color_BD;
  border-left: 1px solid $Color_BD;
  border-right: 1px solid $Color_BD;
  background-color: $Color_FG;
  min-width: 55px;
  padding: 3px;
  border-top-left-radius: $Border2;
  border-top-right-radius: $Border2;
}

QTabBar::tab:selected, QTabBar::tab:hover {
  color: $Color_font;
  border-top: 1px solid $Color_BD;
  border-left: 1px solid $Color_BD;
  border-right: 1px solid $Color_BD;
  background-color: $Color_BG;
}


/*===================================================*/
QComboBox {
  color: $Color_font;
  border: 1px solid $Color_BD;
  background-color: $Color_BG;
}

QComboBox QAbstractItemView  {
  color: $Color_font;
  background-color: $Color_BG;
}

QComboBox:disabled {
  color: $Color_BD;
  border: 1px solid $Color_BD;
  background-color: $Color_BG;
}


/*===================================================*/
QRadioButton {
  color: $Color_font;
  background-color: $Color_FG;
}


/*===================================================*/
QToolBar {
  spacing: 1px;
  background-color: $Color_BG;
  border-bottom: 1px solid $Color_BD;
  border-right: 1px solid $Color_BD;
  qproperty-iconSize: $IconSize;
  min-height: 30px;
}

QToolBar QToolButton {
  background-color: transparent;
  border: 1px solid $Color_BD;
  border-radius: $Border2;
  padding-left: 1px;
}

QToolBar QToolButton:hover {
  background-color: $Color_toolbar_hover;
  border: 1px solid $Color_BD;
  border-radius: $Border2;
  padding-left: 1px;
}

QToolBar QToolButton:disabled {
  opacity: 0.1;
  border: 1px solid $Color_BD;
  border-radius: $Border2;
  padding-left: 1px;
}


/*===================================================*/
QCheckBox {
  color: $Color_font;
}


/*===================================================*/
QSlider {
  border-radius: 10px;
}
QSlider::groove:horizontal {
  height: 4px;
  background-color: $Color_TB;
}

QSlider::handle:horizontal{
  background-color: $Color_BT;
  width: 14px;
  height: 14px;
  margin:-5px 0;
  border-radius: 7px;
}
QSlider::sub-page:horizontal{
  background: $Color_TB;
}

QSlider::handle:horizontal:hover {
  background-color: $Color_TB;
}

QSlider::handle:horizontal:pressed {
  background-color: $Color_TB;
}


QSlider::handle:horizontal:disabled {
  background-color: $Color_BT;
}

QSlider::groove:horizontal:disabled {
  background-color: $Color_BT;
}

QSlider::sub-page:horizontal:disabled{
  background-color: $Color_BT;
}


QSpinBox {
  color: $Color_font;
  border: 1px solid $Color_BD;
  background-color: $Color_BG;
}

QSpinBox:disabled {
  color: #AAAAAA;
  border: 1px solid $Color_BD;
  background-color: $Color_BG;
}
