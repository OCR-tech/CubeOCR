  color: $Color_BD;
  border: 1px solid $Color_BD;
  border-radius: $Border2;
  background-color: $Color_FG;
}

QPushButton::hover {
  border: 1px solid rgba(120,185,255,250);
  background-color: rgba(120,185,255,25);
}

QPushButton::pressed {
  border: 1px solid rgba(120,185,255,250);
  background-color: rgba(100,125,255,35);
}



