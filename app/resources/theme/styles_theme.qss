  border-bottom-left-radius: $Border0;
  border-bottom-right-radius: $Border0;
}


/*===================================================*/
QStatusBar#sb1 {
  background-color: transparent;
  margin-left: 1px;
  border-top: none;
  border-left: 1px solid $Color_TB;
  border-right: 1px solid $Color_TB;
  border-bottom: 1px solid $Color_TB;
  border-bottom-left-radius: $Border5;
  border-bottom-right-radius: $Border5;
}

/*===================================================*/
QStatusBar#sb1::item {
  border: none;
}


/*===================================================*/
 QSizeGrip {
  width: 18px;
