}


/*===================================================*/
#btn1, #btn2 {
  min-width: 65px;
  min-height: 17px;
  max-width: 65px;
  max-height: 17px;
  font-family: Arial;
  color: $Color_font;
  border: 1px solid $Color_BD;
  border-radius: 8px;
  background: qradialgradient(cx:0, cy:0, radius: 0.63, fx:0.1, fy:0.1, stop:0 $Color_BG, stop:1 $Color_btn_gradient2);
}

#btn1::hover, #btn2::hover {
  background: qradialgradient(cx:0, cy:0, radius: 0.63, fx:0.1, fy:0.1, stop:0 $Color_BG, stop:1 $Color_btn_gradient3);
}

#btn1::pressed, #btn2::pressed {
  background: qradialgradient(cx:0, cy:0, radius: 0.63, fx:0.1, fy:0.1, stop:0 $Color_BG, stop:1 $Color_btn_gradient4);
}


/*===================================================*/
#btn3, #btn4, #btn5, #btnMinimize, #btnRestore, #btnMaximize, #btnClose {
  min-width: 17px;
  min-height: 17px;
  max-width: 17px;
  max-height: 17px;
  border-radius: 8px;
}


/*===================================================*/
#btn3 {
  background: qradialgradient(cx:0, cy:0, radius: 0.63, fx:0.1, fy:0.1, stop:0 white, stop:1 rgb(85, 195, 225));
}

#btn3::hover {
  background-color: rgb(85, 145, 225);
  border-image: url(":/resources/icon/titlebar/btn_caret1.png");
}

#btn3::pressed {
  background-color: rgb(85, 125, 225);
  border-image: url(":/resources/icon/titlebar/btn_caret1.png");
}


/*===================================================*/
#btn4 {
  background: qradialgradient(cx:0, cy:0, radius: 0.63, fx:0.1, fy:0.1, stop:0 white, stop:1 rgb(85, 195, 225));
}

#btn4::hover {
  background-color: rgb(85, 145, 225);
  border-image: url(":/resources/icon/titlebar/btn_caret3.png");
}

#btn4::pressed {
  background-color: rgb(85, 125, 225);
  border-image: url(":/resources/icon/titlebar/btn_caret3.png");
}


/*===================================================*/
#btn5 {
  background: qradialgradient(cx:0, cy:0, radius: 0.63, fx:0.1, fy:0.1, stop:0 white, stop:1 rgb(185,185,185));
}

#btn5::hover {
  background-color: rgb(135,135,135);
  border-image: url(":/resources/icon/titlebar/btn_theme1.png");
}

#btn5::pressed {
  background-color: rgb(115,115,115);
  border-image: url(":/resources/icon/titlebar/btn_theme1.png");
}


/*===================================================*/
#btnMinimize {
  background: qradialgradient(cx:0, cy:0, radius: 0.63, fx:0.1, fy:0.1, stop:0 white, stop:1 hsv(38, 218, 253));
}

#btnMinimize::hover {
  background-color: hsv(38, 218, 203);
  border-image: url(":/resources/icon/titlebar/btn_minimize1.png");
}

#btnMinimize::pressed {
  background-color: hsv(38, 218, 153);
  border-image: url(":/resources/icon/titlebar/btn_minimize1.png");
}


/*===================================================*/
#btnRestore, #btnMaximize {
  background: qradialgradient(cx:0, cy:0, radius: 0.63, fx:0.1, fy:0.1, stop:0 white, stop:1 hsv(123, 204, 198));
}

#btnRestore::hover, #btnMaximize::hover {
  background-color: hsv(123, 204, 148);
  border-image: url(":/resources/icon/titlebar/btn_maximize1.png");
}

#btnRestore::pressed, #btnMaximize::pressed {
  background-color: hsv(123, 204, 98);
  border-image: url(":/resources/icon/titlebar/btn_maximize1.png");
}


/*===================================================*/
#btnClose {
   background: qradialgradient(cx:0, cy:0, radius: 0.63, fx:0.1, fy:0.1, stop:0 white, stop:1 hsv(0, 182, 252));
}

#btnClose::hover {
  background-color: hsv(0, 182, 202);
  border-image: url(":/resources/icon/titlebar/btn_close1.png");
}

#btnClose::pressed {
  background-color: hsv(0, 182, 152);
  border-image: url(":/resources/icon/titlebar/btn_close1.png");
}


